{{#each rows as |row|}}
  <tr class="{{if row.isSelected 'selected'}}">
    {{#if enableSelection}}
      <td class="center">{{md-check checked=row.isSelected}}</td>
    {{/if}}
    {{#if hasBlock}}
      {{yield row}}
    {{else}}
      {{default-data-table-content-body item=row data-table=data-table}}
    {{/if}}
  </tr>
{{/each}}
