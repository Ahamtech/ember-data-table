{{#each wrappedItems as |wrapper index|}}
  <tr class="{{if (contains wrapper.item data-table.selection) 'selected'}}">
    {{#if enableSelection}}
      <td class="center">{{md-check action="updateSelection" checked=wrapper.isSelected}}</td>
    {{/if}}
    {{#if enableLineNumbers}}
      <td>{{add index offset}}</td>
    {{/if}}
    {{#if hasBlock}}
      {{yield wrapper.item}}
    {{else}}
      {{default-data-table-content-body item=wrapper.item data-table=data-table}}
    {{/if}}
  </tr>
{{/each}}
